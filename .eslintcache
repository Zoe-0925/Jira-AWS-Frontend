btn\" onClick={handleClose}>\n                <CloseIcon />\n            </IconButton>\n        </MuiDialogTitle>\n    )\n}\n\nexport const DropDownMenu = ({ anchorEl, isOpen, handleMenuClose, children }) => (\n    <Menu\n        anchorEl={anchorEl}\n        anchorOrigin={{ vertical: 'top', horizontal: 'right' }}\n        id={uuidv4()}\n        keepMounted\n        transformOrigin={{ vertical: 'top', horizontal: 'right' }}\n        open={isOpen}\n        onClose={handleMenuClose}\n    >\n        {children}\n    </Menu>\n)","/Users/zoezhang/Downloads/React/JIra-client-serverless-copy/src/Components/Shared/Feedback.js",["270","271"],"/Users/zoezhang/Downloads/React/JIra-client-serverless-copy/src/Components/Shared/EditableText.js",["272"],"/Users/zoezhang/Downloads/React/JIra-client-serverless-copy/src/Components/Issues/IssueAddEpic.js",["273"],"/Users/zoezhang/Downloads/React/JIra-client-serverless-copy/src/Components/Comment/CommentHOC.js",["274","275"],"import React, { useState, useEffect } from 'react'\nimport { useSelector, useDispatch } from \"react-redux\"\nimport Pusher from 'pusher-js';\nimport CommentBox from \"./CommentBox\"\nimport { selectCommentByIssue } from \"../../Reducers/Selectors\"\n\nrequire('dotenv').config()  //Enable access to the \".env\" file\n\nexport default function CommentHOC({ issueId }) {\n    const [comments, setState] = useState([])\n\n    const data = useSelector(selectCommentByIssue)\n\n    useEffect(() => {\n        const pusher = new Pusher(process.env.REACT_APP_PUSHER_APP_KEY || \"\", {\n            cluster: process.env.REACT_APP_PUSHER_APP_CLUSTER || \"\",\n            encrypted: true,\n        });\n\n        const channel = pusher.subscribe('comments');\n\n        channel.bind('new-comment', data => {\n            setState(prevState => {\n                const { commentList } = prevState;\n                commentList.push(data.comment);\n                //TODO\n                //save the commentList to the store and DB\n\n\n   